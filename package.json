{
  "name": "mysite",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "yarn prettier -w .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "test": "echo no test specified",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "@commitlint/cli/**/yargs-parser": "^18.1.3",
    "@gridsome/transformer-remark/**/minimist": "^1.2.5",
    "@vue/cli-plugin-unit-jest/**/yargs-parser": "^18.1.3"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/tailwind": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@sarissa/pagination": "^1.0.3",
    "daisyui": "^4.11.1",
    "fuse.js": "^6.6.2",
    "luxon": "^3.4.3",
    "theme-change": "^2.5.0",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jest": "^29.5.5",
    "@types/twemoji": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "astro": "^2.9.2",
    "autoprefixer": "^10.4.16",
    "core-js": "^3.37.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "8",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.12.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "9.4.2",
    "typescript": "^5.2.2",
    "yarn-audit-fix": "^10.0.1"
  },
  "engines": {
    "node": ">=18.x"
  }
}
